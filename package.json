{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev --local --persist ./public",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc",
    "wrangler": "wrangler pages dev ./public",
    "lint": "eslint . --ext .ts --ext .tsx",
    "generate-db": "drizzle-kit generate:sqlite --schema=app/database/schema.ts",
    "update-db": "drizzle-kit up:sqlite --schema=app/databse/schema.ts"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.15.0",
    "@remix-run/cloudflare-pages": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "cross-env": "^7.0.3",
    "drizzle-orm": "^0.25.3",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "autoprefixer": "^10.4.14",
    "better-sqlite3": "^8.0.1",
    "drizzle-kit": "^0.17.6",
    "eslint": "^8.39.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.5",
    "wrangler": "^2.15.1"
  },
  "engines": {
    "node": ">=16.13"
  }
}
