{
  "name": "sketchbook-seminar",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run dev:all",
    "dev:all": "concurrently \"npm run dev:next\" \"npm run storybook\"",
    "dev:next": "next dev",
	"dev:build": "next build --no-lint",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "test": "jest",
    "db:start": "docker run --rm --name sketchbook-dev-db -e POSTGRES_PASSWORD=postgres -d postgres",
    "db:stop": "docker stop sketchbook-dev-db",
    "db:migrate": "prisma migrate dev -n local",
    "db:populate": "echo \"Does not exist yet\" && false"
  },
  "dependencies": {
    "@prisma/client": "^4.14.0",
    "@types/node": "20.1.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.1",
    "next": "13.4.1",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.9",
    "@storybook/addon-interactions": "^7.0.9",
    "@storybook/addon-links": "^7.0.9",
    "@storybook/addon-styling": "^1.0.6",
    "@storybook/blocks": "^7.0.9",
    "@storybook/nextjs": "^7.0.9",
    "@storybook/react": "^7.0.9",
    "@storybook/testing-library": "^0.0.14-next.2",
    "concurrently": "^8.0.1",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-storybook": "^0.6.12",
    "storybook": "^7.0.9",
    "@types/jest": "^29.5.1",
    "jest": "^29.5.0",
    "prisma": "^4.14.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1"
  }
}
